<UserControl x:Class="GroupProject.Pages.Admin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GroupProject.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="1000" Width="1500">
    <Grid>
        <Image x:Name="___GroupProject_component_Final_logo_for_group_project_jpg" Height="66.666" Margin="0,10,10,0" Source="/GroupProject;component/Final_logo_for_group_project.jpg" Stretch="Fill" VerticalAlignment="Top" HorizontalAlignment="Right" Width="98"/>
        <Button x:Name="Logout_button" Content="Logout" HorizontalAlignment="Left" Height="20" Margin="10,10,0,0" VerticalAlignment="Top" Width="56" FontSize="11"/>
        <Button x:Name="Submit_Button" Content="Submit" HorizontalAlignment="Right" Height="22" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="83.334"/>
        <Button x:Name="Executive_Button" Content="Executive" HorizontalAlignment="Left" Height="22" Margin="10,0,0,10" VerticalAlignment="Bottom" Width="70"/>
        <Grid Margin="66.001,69.716,121.666,73.285">
            <TreeView x:Name="treeView" Margin="0,27.999,0,0" Style="{DynamicResource SimpleTreeView}"/>
            <TextBox x:Name="textBox" Height="27.999" Style="{DynamicResource SimpleTextBox}" TextWrapping="Wrap" Text="Orders Awaiting Authorisation" VerticalAlignment="Top" Background="#FFEAD827" BorderBrush="{x:Null}"/>

        </Grid>

    </Grid>
</UserControl>
